  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "local": "rm -rf dist && tsc && npx serverless offline --config serverless.local.yml --noTimeout",
